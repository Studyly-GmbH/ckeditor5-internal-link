{
  "name": "@samhammer/ckeditor5-internallink-plugin",
  "version": "1.0.13",
  "description": "A generic ckeditor plugin to create link tags containing an internal identifier instead of a url.",
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "ckeditor 5",
    "ckeditor5-feature"
  ],
  "dependencies": {
    "@ckeditor/ckeditor5-core": "^35.0.1",
    "@ckeditor/ckeditor5-engine": "^35.0.1",
    "@ckeditor/ckeditor5-ui": "^35.0.1",
    "awesomplete": "file:awesomplete",
    "axios": "^1.1.3"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-editor-classic": "^35.0.1",
    "@ckeditor/ckeditor5-enter": "^35.0.1",
    "@ckeditor/ckeditor5-heading": "^35.0.1",
    "@ckeditor/ckeditor5-paragraph": "^35.0.1",
    "@ckeditor/ckeditor5-typing": "^35.0.1",
    "@ckeditor/ckeditor5-undo": "^35.0.1",
    "@ckeditor/ckeditor5-utils": "^35.0.1",
    "eslint": "^7.1.0",
    "eslint-config-ckeditor5": "^3.1.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6"
  },
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.0.0"
  },
  "files": [
    "lang",
    "src",
    "theme"
  ],
  "scripts": {
    "lint": "eslint --quiet src/**/*.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --quiet"
    ]
  },
  "eslintIgnore": [
    "src/lib/**",
    "packages/**"
  ]
}
